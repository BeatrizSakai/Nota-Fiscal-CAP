<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <Dialog
      id="uploadCsvDialog"
      title="Importar CSV"
      class="sapUiContentPadding"
      draggable="true"
      resizable="true"
      contentWidth="25rem">
      
    <VBox id ="algo123124">
      <unified:FileUploader
          id="fileUploader"
          xmlns:unified="sap.ui.unified"
          fileType="csv"
          width="100%"
          placeholder="Escolha o arquivo CSVâ€¦" />
    </VBox>

    <beginButton>
      <Button id ="algoa13123124"
          text="Enviar"
          type="Emphasized"
          press="onConfirmUpload" />
    </beginButton>

    <endButton>
      <Button id ="ala13go123124"
          text="Cancelar"
          press="onCancelUpload" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>
