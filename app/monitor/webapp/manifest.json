{
  "_version": "1.65.0",
  "sap.app": {
    "id": "monitoramento.monitor",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.18.2",
      "toolsId": "f1670874-21e7-4b11-9f04-6a1aadd46d5c"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/nota-fiscal/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.138.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "monitoramento.monitor.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {
        "flexibleColumnLayout": {
          "defaultTwoColumnLayoutType": "TwoColumnsBeginExpanded",
          "defaultThreeColumnLayoutType": "ThreeColumnsMidExpanded"
        },
        "routerClass": "sap.f.routing.Router"
      },
      "routes": [
        {
          "pattern": ":?query:",
          "name": "NotaFiscalServicoMonitorList",
          "target": [
            "NotaFiscalServicoMonitorList"
          ]
        },
        {
          "name": "NotaFiscalServicoMonitorObjectPage",
          "pattern": "NotaFiscalServicoMonitor({NotaFiscalServicoMonitorKey}):?query:",
          "target": [
            "NotaFiscalServicoMonitorList",
            "NotaFiscalServicoMonitorObjectPage"
          ]
        }
      ],
      "targets": {
        "NotaFiscalServicoMonitorList": {
          "type": "Component",
          "id": "NotaFiscalServicoMonitorList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/NotaFiscalServicoMonitor",
              "variantManagement": "Page",
              "navigation": {
                "NotaFiscalServicoMonitor": {
                  "detail": {
                    "route": "NotaFiscalServicoMonitorObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "GridTable",
                    "enableExport": true,
                    "selectAll": false,
                    "selectionMode": "Multi",
                    "widthIncludingColumnHeader": true,
                    "personalization": {
                      "column": false,
                      "filter": true,
                      "sort": true
                    },
                    "selectionLimit": 1
                  },
                  "columns": {
                    "LogIconColumn": {
                      "header": "Log",
                      "width": "3.75rem",
                      "horizontalAlign": "Center",
                      "position": {
                        "anchor": "DataField::idAlocacaoSAP",
                        "placement": "Before"
                      },
                      "template": "monitoramento.monitor.ext.fragment.LogIconColumn"
                    },
                    "DataField::idAlocacaoSAP": {
                      "width": "10em"
                    },
                    "DataField::logErroFlag": {
                      "width": "4em"
                    },
                    "DataField::chaveDocumentoMae": {
                      "width": "10em"
                    },
                    "DataField::chaveDocumentoFilho": {
                      "width": "10em"
                    },
                    "DataField::orderIdPL": {
                      "width": "10em"
                    },
                    "DataField::status": {
                      "width": "4em"
                    },
                    "DataField::numeroNfseServico": {
                      "width": "7em"
                    },
                    "DataField::dataEmissaoNfseServico": {
                      "width": "9em"
                    },
                    "DataField::chaveAcessoNfseServico": {
                      "width": "8em"
                    },
                    "DataField::codigoVerificacaoNfse": {
                      "width": "8em"
                    },
                    "DataField::serieNfseServico": {
                      "width": "4em"
                    },
                    "DataField::numeroDocumentoMIRO": {
                      "width": "8em"
                    },
                    "DataField::valorBrutoNfse": {
                      "width": "10em"
                    },
                    "DataField::valorLiquidoFreteNfse": {
                      "width": "10em"
                    },
                    "DataField::valorEfetivoFrete": {
                      "width": "10em"
                    },
                    "DataField::tipoMensagemErro": {
                      "width": "0em"
                    }
                  },
                  "actions": {
                    "DataFieldForAction::NotaFiscalService.rejeitarFrete": {
                      "afterExecution": {
                        "enableAutoScroll": true
                      }
                    },
                    "DataFieldForAction::NotaFiscalService.avancarStatusNFs": {
                      "afterExecution": {
                        "enableAutoScroll": true
                      }
                    },
                    "DataFieldForAction::NotaFiscalService.voltarStatusNFs": {
                      "afterExecution": {
                        "enableAutoScroll": true
                      }
                    },
                    "VerLogs": {
                      "press": "monitoramento.monitor.ext.controller.VerLogs.onVerLogs",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "Ver Logs",
                      "position": {
                        "placement": "After",
                        "anchor": "DataFieldForAction::NotaFiscalService.voltarStatusNFs"
                      }
                    },
                    "printbtn": {
                      "press": "monitoramento.monitor.ext.controller.Print.onPressPrint",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "Imprimir"
                    }
                  }
                }
              },
              "initialLoad": "Enabled",
              "content": {
                "header": {
                  "actions": {
                    "UploadCSV": {
                      "press": "monitoramento.monitor.ext.controller.UploadCSV.onUploadCSV",
                      "visible": true,
                      "enabled": true,
                      "text": "Importar CSV"
                    }
                  }
                }
              }
            }
          },
          "controlAggregation": "beginColumnPages",
          "contextPattern": ""
        },
        "NotaFiscalServicoMonitorObjectPage": {
          "type": "Component",
          "id": "NotaFiscalServicoMonitorObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "navigation": {},
              "contextPath": "/NotaFiscalServicoMonitor",
              "variantManagement": "None",
              "content": {
                "header": {
                  "anchorBarVisible": false,
                  "visible": true
                }
              },
              "editableHeaderContent": false
            }
          },
          "controlAggregation": "midColumnPages",
          "contextPattern": "/NotaFiscalServicoMonitor({NotaFiscalServicoMonitorKey})"
        }
      }
    },
    "rootView": {
      "viewName": "sap.fe.templates.RootContainer.view.Fcl",
      "type": "XML",
      "async": true,
      "id": "appRootView"
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}